{
  "expo": {
    "name": "OpenAgents Chat",
    "slug": "openagents-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.openagents.chat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.openagents.chat"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "webpack",
      "publicPath": "/chat/"
    }
  }
}
